    {
     "data": {
      "text/plain": [
       "1928"
      ]
     },
     "execution_count": 18,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "india_first_gold_year=df[(df.Country =='IND') & (df.Medal =='Gold')][['Year','Medal']]\n",
    "india_first_gold_year=india_first_gold_year.groupby('Year').count().reset_index()\n",
    "india_first_gold_year=india_first_gold_year.sort_values(by='Year',ascending='True').loc[0,'Year']\n",
    "india_first_gold_year\n"
   ]
